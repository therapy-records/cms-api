{
  "name": "express-mongoose-es6-rest-api",
  "version": "1.0.0",
  "description": "A Boilerplate application for building REST APIs using express, mongoose in ES6 with code coverage",
  "author": "Kunal Kapadia <kunalkapadia12@gmail.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=6.13.7",
    "yarn": ">=1.6.0"
  },
  "scripts": {
    "start": "node index.js",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test-with-coverage": "nyc --reporter=html --reporter=text yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:KunalKapadia/express-mongoose-es6-rest-api.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "apollo-server-express": "2.9.9",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "3.4.6",
    "body-parser": "1.19.0",
    "cloudinary": "1.21.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "^2.4.5",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "4.17.1",
    "express-jwt": "5.1.0",
    "express-validation": "1.0.2",
    "express-winston": "3.2.0",
    "graphql": "14.5.8",
    "helmet": "3.1.0",
    "http-status": "^1.3.2",
    "joi": "10.0.6",
    "jsonwebtoken": "7.1.9",
    "method-override": "3.0.0",
    "mongoose": "5.10.2",
    "morgan": "1.9.1",
    "passport": "0.3.2",
    "passport-jwt": "2.2.1",
    "path-to-regexp": "6.1.0",
    "run-sequence": "2.2.1",
    "winston": "3.2.1",
    "woke-dyno": "0.0.4"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-spies": "1.0.0",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-import": "2.18.0",
    "eslint-watch": "5.1.2",
    "ghooks": "^2.0.4",
    "isparta": "4.1.1",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "supertest": "4.0.2"
  },
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "yarn lint"
    }
  }
}
